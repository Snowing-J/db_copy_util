<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.patrick.dbcopy.mapper.TeachersMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.patrick.dbcopy.bean.Teachers">
        <id column="teacher_id" property="teacherId" />
        <result column="teacher_name" property="teacherName" />
        <result column="teacher_school" property="teacherSchool" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        teacher_id, teacher_name, teacher_school
    </sql>

</mapper>
